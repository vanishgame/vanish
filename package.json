{
	"name": "vanish",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"test": "jest",
		"test:watch": "jest --watch",
		"lint": "echo 'tslint...' && tslint --project . 'src/**/*.{ts,tsx}'",
		"tsc-diagnostics": "echo 'tsc...' && tsc --diagnostics",
		"check": "run-s lint tsc-diagnostics",
		"serve": "run-s parcel:serve",
		"parcel:serve": "parcel src/index.html --hmr-port 38483",
		"clean": "rm -rf dist .cache src/dist src/.cache src/config.ts .env",
		"build": "parcel build src/index.html"
	},
	"author": "Georges Cosson, Quentin Oliveira Da Silva",
	"license": "private",
	"dependencies": {
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-redux": "^7.2.0",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0"
	},
	"jest": {
		"preset": "ts-jest",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"globals": {
			"ts-jest": {
				"diagnostics": {
					"warnOnly": true,
					"pathRegex": "\\.(spec|test)\\.ts$"
				}
			}
		}
	},
	"devDependencies": {
		"@types/jest": "^25.1.4",
		"@types/lodash": "^4.14.149",
		"@types/react": "^16.9.22",
		"@types/react-dom": "^16.9.5",
		"@types/react-hot-loader": "^4.1.1",
		"@types/react-redux": "^7.1.7",
		"@types/redux": "^3.6.0",
		"@types/redux-thunk": "^2.1.0",
		"jest": "^25.1.0",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.4",
		"react-hot-loader": "^4.12.19",
		"redux-devtools-extension": "^2.13.8",
		"sass": "^1.26.1",
		"ts-jest": "^25.2.1",
		"tslint": "^6.0.0",
		"tslint-react": "^4.2.0",
		"typescript": "^3.8.2"
	}
}
